{
  "name": "log1x/acf-fluent-helpers",
  "type": "package",
  "license": "MIT",
  "description": "Simple set of helper functions for ACF Fluent auto-loaded with Composer using generic function names.",
  "authors": [
    {
      "name": "Brandon Nifong",
      "email": "brandon@tendency.me"
    }
  ],
  "keywords": [
    "acf",
    "advanced-custom-fields",
    "acf-fluent"
  ],
  "support": {
    "issues": "https://github.com/log1x/acf-fluent-helpers/issues"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ]
  },
  "require": {
    "php": ">=7.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.4"
  },
  "scripts": {
    "lint": [
      "phpcs --ignore=vendor --extensions=php --standard=PSR12 ."
    ]
  }
}
